{
  "name": "@paybyrd/card-collect",
  "version": "1.0.0",
  "description": "Paybyrd's tool to aid in the creation of credit card info collect forms",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paybyrd/card-collect-js.git"
  },
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "build": "microbundle --define VGS_CNAME='token.paybyrd.com',VGS_VAULTID='tntsz1j9tj1',VGS_ENV='live-eu-1'",
    "start": "microbundle watch --define VGS_CNAME='tokensandbox.paybyrd.com',VGS_VAULTID='tnt3uowyamd',VGS_ENV='sandbox'",
    "dev": "microbundle --define VGS_CNAME='tokensandbox.paybyrd.com',VGS_VAULTID='tnt3uowyamd',VGS_ENV='sandbox'",
    "stage": "microbundle --define VGS_CNAME='tokensandbox.paybyrd.com',VGS_VAULTID='tnt3uowyamd',VGS_ENV='sandbox'"
  },
  "keywords": [
    "card",
    "collect",
    "security",
    "paybyrd"
  ],
  "files": [
    "dist",
    "src"
  ],
  "author": "tiago.resende@paybyrd.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paybyrd/card-collect-js/issues"
  },
  "homepage": "https://github.com/paybyrd/card-collect-js#readme",
  "dependencies": {
    "@vgs/collect-js": "^0.6.1"
  },
  "devDependencies": {
    "microbundle": "^0.15.1",
    "typescript": "^4.9.5"
  }
}
